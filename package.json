{
  "name": "safeapi-bridge",
  "version": "1.0.0",
  "description": "Secure API proxy server that protects your AI API keys from client-side exposure",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "reset:daily": "node scripts/scheduler/reset-quotas.js --daily",
    "reset:monthly": "node scripts/scheduler/reset-quotas.js --monthly",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "render:configure": "node scripts/render/configure.js",
    "test:remote:gemini": "node scripts/remote/test-gemini.js",
    "postinstall": "prisma generate",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:security": "jest --testPathPattern=tests/security"
  },
  "keywords": [
    "proxy",
    "api",
    "openai",
    "gemini",
    "claude",
    "jwt",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "@prisma/client": "^5.8.0",
    "crypto-js": "^4.2.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8"
  }
}
